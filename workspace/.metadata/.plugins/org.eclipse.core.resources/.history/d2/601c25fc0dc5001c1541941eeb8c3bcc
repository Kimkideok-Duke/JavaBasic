package javaexp.a02_start;

public class A06_CharCodeExp {
	
	public static void main(String[] args) {
		// 1)
		char a = 'a', A = 'A', z = 'z', Z = 'Z';
		System.out.println("a = " + (int)a);
		System.out.println("A = " + (int)A);
		System.out.println("z = " + (int)z);
		System.out.println("Z = " + (int)Z);
	}

}
